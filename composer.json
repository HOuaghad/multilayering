{
    "name": "hamzaouaghad/multilayering",
    "description": "A tiny laravel package to speed up the workflow with the multilayering convetion",
    "keywords": [
        "hamzaouaghad",
        "hamza",
        "ouaghad",
        "laravel",
        "multilayering",
        "layers",
        "seperation-of-concerns",
        "soc",
    ],
    "homepage": "https://github.com/HOuaghad/multilayering",
    "license": "MIT",
    "authors": [
        {
            "name": "Hamza Ouaghad",
            "email": "ouaghad.hamza@gmail.com",
            "homepage": "not-available",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Hamzaouaghad\\Multilayering\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hamzaouaghad\\Multilayering\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
